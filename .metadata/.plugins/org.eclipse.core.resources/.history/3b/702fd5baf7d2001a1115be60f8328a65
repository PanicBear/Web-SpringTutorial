package kr.co.netmania.diexam01;

public class UserBean {

//	1) 기본 생성자
//	2) private 필드
//	3) getter, setter 메서드

	private String name;
	private int age;
	private boolean male;

	public UserBean() {
	}

	public UserBean(String name, int age, boolean male) {
		super();
		this.name = name;
		this.age = age;
		this.male = male;
	}

}
